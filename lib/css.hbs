@font-face {
	font-family: "{{fontName}}";
	src: {{{src}}};
}

.{{baseSelector}} {
	font-family: {{fontName}} !important;
	font-style: normal;
	font-weight: normal !important;
	line-height: 1;
	vertical-align: middle;
}

{{#each iconValues}}
.{{../baseSelector}}.{{../classPrefix}}{{@key}}:before {
	content: '{{this.css}}';
}
{{/each}}